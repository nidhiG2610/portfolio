logo {\n            top: 0;\n            #{$opposite-direction}: rem-calc(-8);\n            @media #{$large-up} {\n                #{$opposite-direction}: 0;\n            }\n        }\n    }\n    &:hover {\n        .banner__edit-button--banner {\n            opacity: 1;\n        }\n    }\n    &__logo-container {\n        display: block;\n        position: absolute;\n        z-index: 1;\n        #{$default-float}: 50%;\n        transform: translate(-50%, 0);\n        top: rem-calc(16);\n        @media #{$large-up} {\n            transform: none;\n            #{$default-float}: $logo-offset-horiz-large;\n            top: rem-calc(48);\n        }\n        &:hover {\n            .banner__edit-button--logo {\n                opacity: 1;\n            }\n        }\n    }\n    &__logo {\n        background-position: center;\n        background-size: cover;\n        overflow: hidden;\n        border-radius: 50%;\n        height: $logo-dimension-smmed;\n        width: $logo-dimension-smmed;\n        @media #{$large-up} {\n            height: $logo-dimension-large;\n            width: $logo-dimension-large;\n        }\n    }\n    &__title {\n        position: absolute;\n        margin: 0;\n        bottom: 0;\n        padding: rem-calc(0 30);\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        text-align: center;\n        width: 100%;\n        color: $text-color-inverse;\n        height: rem-calc(46);\n        line-height: rem-calc(46);\n        font-size: rem-calc(20);\n        @media #{$medium-up} {\n            height: rem-calc(60);\n            line-height: rem-calc(60);\n            font-size: $font-size-large;\n            // Push the title down a bit on med breakpoint so it doesn't touch the logo\n            padding-top: rem-calc(4);\n        }\n\n        @media #{$large-up} {\n            text-align: initial;\n            font-size: $font-size-xlarge;\n            padding-#{$opposite-direction}: 0;\n            padding-#{$default-float}: $title-offset-left-large;\n            padding-top: 0;\n        }\n\n        &:before {\n            content: '';\n            position: absolute;\n            #{$default-float}: 0;\n            top: 0;\n            height: 100%;\n            width: 100%;\n            opacity: 0.75;\n            background: linear-gradient(to bottom, rgba(0, 0, 0, 0), rgba(0, 0, 0, 1));\n            // Set a z-index on this background gradient so that the content is\n            // sure to render on top of it. Content must also have a z-index > 0\n            z-index: 0;\n        }\n        // Make sure content renders above the background gradient\n        & > * {\n            z-index: 1;\n        }\n\n        .inline-edit-container--header {\n            height: 100%;\n            display: flex;\n            flex-direction: row;\n            justify-content: center;\n            align-items: center;\n            font-size: rem-calc(20);\n\n            @media #{$medium-up} {\n                font-size: $font-size-large;\n            }\n            @media #{$large-up} {\n                font-size: $font-size-xlarge;\n                justify-content: flex-start;\n            }\n            &__input, &__text {\n                color: $text-color-inverse;\n                text-align: center;\n                padding: rem-calc(2);\n            }\n            &__edit {\n                color: $text-color-inverse;\n            }\n            &__link {\n                line-height: rem-calc(42);\n                justify-content: center;\n\n                @media #{$large-up} {\n                    line-height: initial;\n                    justify-content: start;\n                }\n            }\n            &__validation {\n                justify-content: center;\n            }\n\n            .inline-edit-text-field {\n                width: 100%;\n                height: 100%;\n                min-width: rem-calc(100);\n                max-width: rem-calc(400);\n         